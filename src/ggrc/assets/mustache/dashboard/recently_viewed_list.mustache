{{#list}}
<li class="{{model.category.toLowerCase}}" data-model="true" {{data 'model'}}>
  <a href="{{viewLink}}" class="show-extended">
    <div class="row-fluid">
      <div class="span11">
        <div class="oneline">
          <i class="fa fa-{{model.table_singular}} color"></i>
          {{firstnonempty title name email}}
        </div>
      </div>
    </div>
  </a>

</li>
{{/list}}
